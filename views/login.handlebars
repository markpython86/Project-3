<div id="home">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2 login">
        <div id="signUp">Login to Minima<strong>list</strong>.</div>
        <form class="clearfix mb-4" action="POST">
          <div class="form-group">
            <label for="username">Username</label>
            <input type="text" id="username" name="username" class="form-control" aria-describedby="example-text" placeholder="Username">
          </div>
          <div class="form-group">
            <label for="password">Password</label>
            <input class="form-control" type="password" id="password" aria-describedby="example-description" placeholder="Password"></input>
          </div>
          <div> 
          <div class="row">
            <div class="col-sm">
              <button id="submit" class="btn btn-primary">Log In</button>
            </div>
            <div class="col-sm">
              <span id="clickHere" class="float-right">Don't have an account?<br><a href="/signup"> <strong>Click here</strong></a> to
                create one.</span>
            </div>
          </div>
          </div>
        </form>
    
      </div>
    </div>
  </div>
</div>

